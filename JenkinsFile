pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            
                int main()
                {
                     int num=2;
                     if(num%2==0)
                        cout << "Even";
                     else
                        cout << "Odd";
                    return 0;
                }
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing '
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
