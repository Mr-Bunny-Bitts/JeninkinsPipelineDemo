pipeline {
    agent any

    stages {
    
        stage('Clear') {
            steps { 
               echo 'hello'
               
               sh 'pyinstaller --onefile -w 'Hello.py' '
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
                //bat 'build.bat'  
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
                //bat 'deploy.bat'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing '
                //bat 'pipelinetest.bat'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
                 //bat 'release.bat'
            }
        }
    }
}
