pipeline {
    agent any

    stages {
    
        stage('Clear') {
            steps {
                bat '''for /f %%A in ('dir "D:\\JenkinsHome\\workspace\\PipelineOne\\[REALEASE]" /a-d-s-h /b ^| find /v /c ""') do set a=%%A
                set "a=%globalparam1%"
                goto :aCheck
                :aPrompt
                set /p "a=Enter Choice: "
                :aCheck
                if "%a%"=="" goto :aPrompt
                echo %a%
                if "%cnt%" >="10" del D:\\JenkinsHome\\workspace\\PipelineOne\\[REALEASE]\\TEST*.zip'''
                
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
                bat 'build.bat'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
                bat 'deploy.bat'
 
            }
        }
        stage('Test') {
            steps {
                echo 'Testing '
                bat 'test.bat'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
                 bat 'release.bat'
             
                
            }
        }
    }
}
