pipeline {
    agent any

    stages {
    
        stage('Clear') {
            steps {
                bat '''dir /a:-d "D:\\JenkinsHome\\workspace\\PipelineOne\\[REALEASE]" | find /c "cnt:" '''
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
                bat 'build.bat'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
                bat 'deploy.bat'
 
            }
        }
        stage('Test') {
            steps {
                echo 'Testing '
                bat 'test.bat'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
                 bat 'release.bat'
             
                
            }
        }
    }
}
