pipeline {
    agent any

    stages {
    
        stage('Clear') {
            steps { 
               echo 'hello'
                bat '''@echo off

                for /f "tokens=* delims=" %%# in ('FileDateFilterJS.bat  "." -hh -5') do (
                 echo deleting  "%%~f#"
                echo del /q /f "%%~f#"
                   )

                //find 'D:\\JenkinsHome\\workspace\\PipelineOne\\[REALEASE]' -mtime +10 -type f -delete
                '''
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
                bat 'build.bat'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
                bat 'deploy.bat'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing '
                bat 'pipelinetest.bat'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
                bat 'release.bat'
            }
        }
    }
}
