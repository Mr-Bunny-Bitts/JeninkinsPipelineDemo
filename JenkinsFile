pipeline {
    agent any

    stages {
    
        stage('Clear') {
            steps {
                
                echo 'hello'
                for /f "delims=" %a in ('dir "D:\\JenkinsHome\\workspace\\PipelineOne\\[REALEASE]" /B /S Test*') do del "%a"
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
                bat 'build.bat'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
                bat 'deploy.bat'
 
            }
        }
        stage('Test') {
            steps {
                echo 'Testing '
                bat 'test.bat'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
                 bat 'release.bat'
             
                
            }
        }
    }
}
