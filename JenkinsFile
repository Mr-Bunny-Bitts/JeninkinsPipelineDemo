pipeline {
    agent any

    stages {
    
        stage('Clear') {
            steps {
                bat '''
                forfiles /m t* /c "cmd /c Del t* " /d -3'''
                
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
                bat 'build.bat'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
                bat 'deploy.bat'
 
            }
        }
        stage('Test') {
            steps {
                echo 'Testing '
                bat 'test.bat'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
                 bat 'release.bat'
             
                
            }
        }
    }
}
