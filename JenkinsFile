pipeline {
    agent any

    stages {
    
        stage('Clear') {
            steps { 
               echo 'hello'
                bat '''forfiles /p D:\\JenkinsHome\\workspace\\PipelineOne\\[REALEASE] /m t* /c "cmd /c Del t* " /d -3
                '''
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
                bat 'build.bat'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
                bat 'deploy.bat'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing '
                bat 'pipelinetest.bat'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
                bat 'release.bat'
            }
        }
    }
}
